	<queryString>
		<![CDATA[SELECT M.IDMEDICAMENTO, M.MEDICAMENTO, M.CANTIDAD || ' ' || U.ABREVIATURA CANTIDAD, P.PRESENTACION, TO_CHAR(M.FECHA_V, 'dd-mm-yyyy') VENCIMIENTO  FROM MEDICAMENTO M 
    INNER JOIN UNIDAD_MED U ON U.IDUNIDAD_MED = M.IDUNIDAD_MED
    INNER JOIN TIPO_MEDICAMENTO TP ON TP.IDTIPO_MED = M.IDTIPO
