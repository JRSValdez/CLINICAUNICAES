	<subDataset name="setAntecedentes" uuid="92834c57-a36a-4ea2-9551-eaa550659aaa">
		<queryString>
			<![CDATA[SELECT ENF.COD_CAT, ENF.ENFERMEDAD FROM ANTECEDENTES_X_CONS A
    INNER JOIN CIE10_ENFERMEDAD ENF ON ENF.COD_ENF = A.COD_ENF
    INNER JOIN CONSULTA C ON C.IDCONSULTA = A.IDCONSULTA
WHERE C.IDPACIENTE = 9 AND C.IDCONSULTA = 9]]>
		</queryString>
		<field name="COD_CAT" class="java.lang.String"/>
		<field name="ENFERMEDAD" class="java.lang.String"/>
	</subDataset>
	<parameter name="idPaciente" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
            C.IDCONSULTA, 
            C.IDPACIENTE, DOC.IDDOCTOR,
            P.PAC_NOMBRE || ' ' || P.PAC_APELLIDO PACIENTE,
            P.PAC_TELEFONO,
            P.PAC_CARNE,
            P.PAC_DOCUMENTO,
            P.PAC_DIRECCION,
            P.PAC_RESPONSABLE,
            P.PAC_RES_TEL,
            DOC.DOC_NOMBRE || ' ' || DOC.DOC_APELLIDO DOCTOR,
            TO_CHAR(C.CONS_FECHA, 'dd-mm-yyyy') FECHA,
            D.MOTIVO,
            D.EF_ABDOMEN,
            D.EF_CABEZA,
            D.EF_CUELLO,
            D.EF_EXTREMIDADES,
            D.EF_TORAX,
            D.FREC_CAR,
            D.PESO,
            D.PRES_ART,
            D.PULSO,
            D.TALLA,
            D.MOTIVO,
            D.RECOMENDACION,
ENF.COD_CAT,
            ENF.ENFERMEDAD
FROM CONSULTA C
    INNER JOIN DET_CONSULTA D ON D.IDCONSULTA = C.IDCONSULTA
    INNER JOIN DOCTOR DOC ON DOC.IDDOCTOR = C.IDDOCTOR
    INNER JOIN PACIENTES P  ON P.IDPACIENTE = C.IDPACIENTE
    INNER JOIN ANTECEDENTES_X_CONS ANT ON ANT.IDCONSULTA = C.IDCONSULTA
    INNER JOIN CIE10_ENFERMEDAD ENF ON ENF.COD_ENF = ANT.COD_ENF
WHERE C.IDPACIENTE = $P{idPaciente}]]>
	</queryString>
